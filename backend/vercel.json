{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "enableWebSocketTraffic": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/socket.io/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
